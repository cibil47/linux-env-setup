{
  "clangd.compilationDatabaseCandidates": ["./", "build", "Debug", "Release"],
  "clangd.arguments": [
    "--background-index",
    "--clang-tidy"
  ],
"languageserver": {
    "asm-lsp": {
      "command": "asm-lsp",
      "filetypes": ["asm", "s", "S", "assembly", "nasm"]
    },
    "bitbake": {
      "command": "bitbake-language-server",
      "filetypes": ["bitbake"]
    },
    "ginko_ls": {
      "command": "ginko_ls",
      "filetypes": ["dts"]
    }
  }
}
